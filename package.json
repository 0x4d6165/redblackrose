    "@marblejs/middleware-logger": "^3.0.1",
    "@most/core": "^1.5.0",
    "@most/scheduler": "^1.2.3",
    "@most/types": "^1.0.2",
    "@types/node": "^13.9.2",
    "fp-ts": "^2.5.3",
    "parceljs": "^0.0.1",
    "rxjs": "^6.5.4",
    "ts-node": "^8.7.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
