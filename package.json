    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "snabbdom-pragma": "^2.8.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "tslint": "^5.18.0",
    "webpack": "4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "3.7.2"
